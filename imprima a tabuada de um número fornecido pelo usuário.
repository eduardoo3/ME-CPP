#include <iostream>

int main() {
    int numero;

    // Solicita um número inteiro ao usuário
    std::cout << "Digite um número inteiro para ver a tabuada: ";
    std::cin >> numero;

    // Imprime a tabuada do número fornecido
    std::cout << "Tabuada do " << numero << ":\n";
    for (int i = 1; i <= 10; ++i) {
        std::cout << numero << " x " << i << " = " << numero * i << std::endl;
    }

    return 0;
}
