#include <iostream>

int main() {
    float celsius, fahrenheit;

    // Solicita a temperatura em graus Celsius ao usuÃ¡rio
    std::cout << "Digite a temperatura em graus Celsius: ";
    std::cin >> celsius;

    // Converte a temperatura para Fahrenheit
    fahrenheit = (celsius * 9/5) + 32;

    // Imprime a temperatura em Fahrenheit
    std::cout << "A temperatura em Fahrenheit Ã©: " << fahrenheit << std::endl;

    return 0;
}
