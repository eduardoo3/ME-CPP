#include <iostream>

int main() {
    int num1, num2, soma;

    std::cout << "Digite o primeiro número inteiro: ";
    std::cin >> num1;

    std::cout << "Digite o segundo número inteiro: ";
    std::cin >> num2;

    soma = num1 + num2;

    std::cout << "A soma dos dois números é: " << soma << std::endl;

    return 0;
}
